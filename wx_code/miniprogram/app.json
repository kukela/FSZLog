{
  "pages": [
    "pages/index/index",
    "pages/installment/installment",
    "pages/logs/logs",
    "pages/sync/sync",
    "pages/imInfo/imInfo"
  ],
  "tabBar": {
    "color": "@tabBarColor",
    "selectedColor": "@tabBarSelectedColor",
    "borderStyle": "@tabBarBorderStyle",
    "backgroundColor": "@tabBarBackgroundColor",
    "list": [{
        "pagePath": "pages/index/index",
        "text": "本月",
        "iconPath": "@iconPathBy",
        "selectedIconPath": "@selectedIconPathBy"
      },
      {
        "pagePath": "pages/installment/installment",
        "text": "分期",
        "iconPath": "@iconPathInstallment",
        "selectedIconPath": "@selectedIconPathInstallment"
      },
      {
        "pagePath": "pages/logs/logs",
        "text": "历史",
        "iconPath": "@iconPathLog",
        "selectedIconPath": "@selectedIconPathLog"
      },
      {
        "pagePath": "pages/sync/sync",
        "text": "同步",
        "iconPath": "@iconPathSync",
        "selectedIconPath": "@selectedIconPathSync"
      }
    ]
  },
  "window": {
    "navigationBarTextStyle": "@navigationBarTextStyle",
    "navigationBarBackgroundColor": "@navigationBarBackgroundColor",
    "backgroundColor": "@backgroundColor"
  },
  "style": "v2",
  "rendererOptions": {
    "skyline": {
      "defaultDisplayBlock": true,
      "disableABTest": true,
      "sdkVersionBegin": "3.0.0",
      "sdkVersionEnd": "15.255.255"
    }
  },
  "componentFramework": "glass-easel",
  "sitemapLocation": "sitemap.json",
  "lazyCodeLoading": "requiredComponents",
  "darkmode": true,
  "themeLocation": "theme.json"
}