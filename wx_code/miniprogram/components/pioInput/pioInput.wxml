<radio-group bindchange="radioChange">
  <label wx:for="{{list}}" wx:key="v" class="{{item.v}}">
    <radio value="{{item.v}}" checked="{{pio == item.v}}" />
    <text>{{item.title}}</text>
  </label>
</radio-group>
<tip-input type="digit" placeholder="请输入价格" model:value="{{p}}" tips="{{tips}}" verifyTips="{{verifyTips}}"></tip-input>